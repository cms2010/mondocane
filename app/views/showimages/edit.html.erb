<h1>Editing showimage</h1>

<% form_for(@showimage) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.label :show %><br />
    <%= f.text_field :show %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @showimage %> |
<%= link_to 'Back', showimages_path %>