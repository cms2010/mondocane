<h1>Listing events</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Subtitle</th>
    <th>Url</th>
    <th>Date1</th>
    <th>Date2</th>
    <th>Address1</th>
    <th>Address2</th>
    <th>City</th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%=h event.title %></td>
    <td><%=h event.subtitle %></td>
    <td><%=h event.url %></td>
    <td><%=h event.date1 %></td>
    <td><%=h event.date2 %></td>
    <td><%=h event.address1 %></td>
    <td><%=h event.address2 %></td>
    <td><%=h event.city %></td>
    <td><%= link_to 'Show', event %></td>
    <td><%= link_to 'Edit', edit_event_path(event) %></td>
    <td><%= link_to 'Destroy', event, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New event', new_event_path %>